{
  "name": "whois-extractor-email-tool",
  "displayName": "WhoisExtractor: Email Tool",
  "version": "1.0.0",
  "description": "Local email extraction from text and files (Chrome extension)",
  "author": "ProgrammerNomad",
  "license": "MIT",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx,css}",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "plasmo": "^0.90.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "^4.0.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.3.1",
    "@types/chrome": "^0.0.268",
    "@types/node": "^20.14.10",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3"
  },
  "manifest": {
    "host_permissions": [],
    "permissions": [
      "storage",
      "offscreen"
    ],
    "commands": {
      "open-tool-page": {
        "suggested_key": {
          "default": "Ctrl+Shift+E"
        },
        "description": "Open WhoisExtractor Tool Page"
      }
    }
  }
}
